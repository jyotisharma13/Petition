<!-- password field should be blank.
we can add profile link and pictures as well -->
<h3> Petition </h3>
<img src ="smoke.jpg" width= "200px" height = "100px">
<h4> please check and update your profile information.</h4>
{{#if error}}
<div class name="error">SORRY! It seems like you did not enter your first and last name. Please let us know who you are.</div>
{{/if}}
<form method="post">
<div> First Name
<input name = "first" placeholder="FirstName">
</div>
<div> Last Name
<input name = "last" placeholder="LastName"autocomplete="off" autofocus>
</div>
<div> Email Address
<input name = "email" placeholder="Email Address"autocomplete="off" autofocus>
</div>
<div> Password
<input name = "password" placeholder="Password"autocomplete="off" autofocus>
</div>
<div> AGE
<input name="age" placeholder="Age">
</div>
<div> City
<input name="city" placeholder="City">
</div>
<div> Homepage
<input name="Homepage" placeholder="Homepage">
</div>
<button> Update </button>
<input name ="_csrf" type="hidden" value="{{csrfToken}}">
</form>
